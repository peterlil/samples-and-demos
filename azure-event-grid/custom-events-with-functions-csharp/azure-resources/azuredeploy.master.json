{
    "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
		"_artifactsLocation": {
			"type": "string"
		},
		"_artifactsLocationSasToken": {
			"type": "securestring"
		}
    },
    "variables": {
        "appInsightsLinkedTemplateFileName": "azuredeploy.appinsights.json"
	},
    "resources": 
    [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "appInsights-linked",
            "dependsOn": [],
			"properties": {
                "mode": "Incremental",
                "templateLink": {
					"uri": "[concat(parameters('_artifactsLocation'), '/', variables('appInsightsLinkedTemplateFileName'), parameters('_artifactsLocationSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
                    "appInsightsName": { "value": "appInsightsName" },
                    "location": { "value": "northeurope" },
                    "functionInitiatorName": { "value": "funnyFunction" }
				}
            }
        }
    ]
}